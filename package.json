{
  "name": "pretty-damn-quick",
  "version": "0.0.1",
  "description": "Run Prettier on your changes only",
  "keywords": [
    "prettier",
    "git",
    "format",
    "changed-files",
    "prettier-plugin",
    "diff"
  ],
  "homepage": "https://github.com/Prog-Jacob/pretty-damn-quick#readme",
  "bugs": {
    "url": "https://github.com/Prog-Jacob/pretty-damn-quick/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Prog-Jacob/pretty-damn-quick.git"
  },
  "license": "MIT",
  "author": "Ahmed Abdelaziz <Ahmed.Abdelaziz.GM@gmail.com> (https://github.com/Prog-Jacob)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "tsc --build tsconfig.build.json --clean",
    "format": "prettier --write .",
    "lint": "eslint --cache --ext .js,.ts --fix .",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "prepublish": "pnpm clean && pnpm build",
    "postpublish": "pinst --enable",
    "release": "np",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --coverage",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.10",
    "@types/jest": "^30.0.0",
    "@types/node": "^18.11.18",
    "@types/yargs": "^17.0.34",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "np": "^7.7.0",
    "pinst": "^3.0.0",
    "prettier": "^3.0.0",
    "pretty-quick": "^4.2.2",
    "ts-jest": "^29.0.5",
    "tsd": "^0.25.0",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "resolutions": {
    "cosmiconfig": "^8.1.3"
  },
  "dependencies": {
    "yargs": "^18.0.0"
  }
}
